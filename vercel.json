{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://calindly-97aea8f91ac2.herokuapp.com/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    }
  ]
}